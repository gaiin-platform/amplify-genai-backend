# Example environment file for local testing
# Copy to .env.dev or .env.local and fill in your values

# Server Configuration
PORT=8080
NODE_ENV=development
STREAM=true

# CORS
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:5173

# AWS Region
DEP_REGION=us-east-1

# Cognito Authentication
COGNITO_USER_POOL_ID=us-east-1_xxxxx
COGNITO_CLIENT_ID=xxxxxxxxxxxxx
IDP_PREFIX=

# S3 Buckets
S3_FILE_TEXT_BUCKET_NAME=amplify-xxx-lambda-dev-file-text
S3_IMAGE_INPUT_BUCKET_NAME=amplify-xxx-lambda-dev-image-input
S3_RAG_INPUT_BUCKET_NAME=amplify-xxx-lambda-dev-rag-input
TRACE_BUCKET_NAME=amplify-xxx-lambda-dev-chat-traces
ASSISTANT_TASK_RESULTS_BUCKET_NAME=amplify-xxx-amplify-js-dev-ast-results
ASSISTANT_LOGS_BUCKET_NAME=amplify-xxx-dev-assistant-chat-logs
S3_GROUP_ASSISTANT_CONVERSATIONS_BUCKET_NAME=amplify-xxx-assistants-dev-group-conversations-content

# DynamoDB Tables
HASH_FILES_DYNAMO_TABLE=amplify-xxx-lambda-dev-hash-files
CHAT_USAGE_DYNAMO_TABLE=amplify-xxx-lambda-dev-chat-usage
REQUEST_STATE_DYNAMO_TABLE=amplify-xxx-amplify-js-dev-request-state
ASSISTANTS_ALIASES_DYNAMODB_TABLE=amplify-xxx-assistants-dev-assistant-aliases
ASSISTANTS_DYNAMODB_TABLE=amplify-xxx-assistants-dev-assistants
API_KEYS_DYNAMODB_TABLE=amplify-xxx-object-access-dev-api-keys
COST_CALCULATIONS_DYNAMO_TABLE=amplify-xxx-lambda-dev-cost-calculations
HISTORY_COST_CALCULATIONS_DYNAMO_TABLE=amplify-xxx-lambda-dev-history-cost-calculations
MODEL_RATE_TABLE=amplify-xxx-chat-billing-dev-model-rates
GROUPS_DYNAMO_TABLE=amplify-xxx-object-access-dev-amplify-groups
GROUP_ASSISTANT_CONVERSATIONS_DYNAMO_TABLE=amplify-xxx-assistants-dev-group-assistant-conversations
ADMIN_DYNAMODB_TABLE=amplify-xxx-admin-dev-admin-configs
AGENT_STATE_DYNAMODB_TABLE=amplify-xxx-agent-loop-dev-agent-state
DATASOURCE_REGISTRY_DYNAMO_TABLE=amplify-xxx-amplify-js-dev-datasource-registry

# SQS
ASSISTANT_QUEUE_NAME=amplify-xxx-amplify-js-dev-assistant-queue

# Secrets Manager
LLM_ENDPOINTS_SECRETS_NAME=your-stage-llm-endpoints-secret
SECRETS_ARN_NAME=dev-amplify-app-secrets

# API Configuration
API_BASE_URL=https://your-api-domain.com

# Feature Flags
TRACING_ENABLED=false
CHANGE_SET_BOOLEAN=false

# Agent Configuration (if applicable)
AGENT_ENDPOINT=https://your-agent-endpoint.com
